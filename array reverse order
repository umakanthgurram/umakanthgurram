#include <stdio.h>

int main() {
    int n,i;
    int num[100];
    printf("enter number of elements:");
    scanf("%d",&n);
    printf("enter elements in the array\n");
    for(i=0;i<n;i++){
        printf("element %d :",i);
        scanf("%d",&num[i]);
    }
    for(i=n-1;i>=0;i--){
        printf("%d",num[i]);
    }
    return 0;
}
